<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="450.0" prefWidth="278.0"
            xmlns="http://javafx.com/javafx/9.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="PROZ.LogInView">
    <children>
        <Label alignment="CENTER" layoutX="40.0" layoutY="67.0"
               text="Ticket Sales System">
            <font>
                <Font name="Leelawadee UI Semilight" size="24.0"/>
            </font>
        </Label>
        <TextField fx:id="login" layoutX="65.0" layoutY="175.0"
                   onKeyPressed="#logInEnterAction"/>
        <PasswordField fx:id="password" layoutX="65.0" layoutY="213.0"
                       onKeyPressed="#logInEnterAction"/>
        <Button fx:id="signUp" layoutX="156.0" layoutY="257.0"
                mnemonicParsing="false" onAction="#signUpAction"
                text="Sign Up"/>
        <Button fx:id="signIn" layoutX="65.0" layoutY="257.0"
                mnemonicParsing="false" onAction="#signInAction"
                text="Sign In"/>
        <Label layoutX="195.0" layoutY="429.0" text="Bartlomiej Kulik">
            <font>
                <Font size="10.0"/>
            </font>
        </Label>
        <Label fx:id="signInFailed" layoutX="76.0" layoutY="306.0"
               text="Sign In failed. Try again." textFill="#b90000"
               visible="false"/>
    </children>
</AnchorPane>
